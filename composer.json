{
    "name": "myint-oo/laravel-storage-wrapper",
    "description": "A wrapper for laravel storage using interface. A small package meant to solve having to do all the for cloud storage setup work in each of the projects",
    "license": "MIT",
    "authors": [
        {
            "name": "TheMyintOo",
            "email": "TheMyintOo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MyintOo\\LaravelStorageWrapper\\": "src/"
        }
    },
    "require": {
        "illuminate/support": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyintOo\\LaravelStorageWrapper\\Providers\\LaravelStorageWrapperServiceProvider"
            ]
        }
    }
}
